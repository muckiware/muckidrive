{
  "name": "muckidrive",
  "version": "0.0.1",
  "description": "muckidrive server system",
  "author": "muckiware",
  "license": "MIT",
  "homepage": "https://github.com/muckiware/muckidrive",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run cleanupdistfolder && npm run copyvarfolder && ng build && nest build",
    "buildWebpack": "tsc -p app_modules/tsconfig.appmodules.json && tsc -p tsconfig.build.json",
    "buildTest": "npm run cleanupdistfolder && npm run copyvarfolder && webpack --config webpack.config.js",
    "start": "npm run cleanupdistfolder && npm run copyvarfolder && ng build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test2": "jest --passWithNoTests --bail",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copyvarfolder": "mkdir -p dist/var && cp -R var/ dist/var",
    "copymasterfolder": "mkdir -p dist/master && cp -R master/ dist/master",
    "cleanupdistfolder": "rm -rf dist && mkdir dist"
  },
  "dependencies": {
    "@angular/animations": "^12.2.15",
    "@angular/common": "^12.2.15",
    "@angular/compiler": "^12.2.15",
    "@angular/core": "^12.2.15",
    "@angular/forms": "^12.2.15",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.15",
    "@angular/platform-browser-dynamic": "^12.2.15",
    "@angular/router": "^12.2.15",
    "@apollo/client": "^3.3.21",
    "@casl/ability": "^5.3.1",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.4",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@types/lodash": "^4.14.172",
    "apollo-angular": "^2.6.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dot-prop": "^6.0.1",
    "faker": "^5.5.3",
    "graphql": "^15.8.0",
    "graphql-tools": "^7.0.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "luxon": "^1.27.0",
    "mkdirp": "^1.0.4",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.0",
    "sqlite3": "^5.0.2",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "write-file-atomic": "^3.0.3",
    "write-yaml-file": "^4.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.14",
    "@angular/cli": "^12.2.14",
    "@angular/compiler-cli": "^12.2.15",
    "@golevelup/ts-jest": "^0.3.1",
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^8.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/dot-prop": "^5.0.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.1",
    "@types/log4js": "^2.3.5",
    "@types/luxon": "^1.27.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.1",
    "@types/write-file-atomic": "^3.0.1",
    "jest": "^26.6.3",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/app_modules/",
      "<rootDir>/tests/"
    ],
    "moduleNameMapper": {
      "appmodules/app_modules/(.*)": "<rootDir>/app_modules/src/$1"
    }
  }
}
